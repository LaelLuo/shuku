            "div.c_description"
        ],
        "category": [
            "div.c_tag:nth-of-type(2) span:nth-of-type(4)"
        ],
        "status": [
            "span:nth-of-type(8)"
        ],
        "update": [
            "div.c_tag:nth-of-type(4) span:nth-of-type(4)"
        ],
        "lastChapter": [
            "div:nth-of-type(4) span:nth-of-type(2)"
        ],
        "link": [
            ".c_subject a@attr->href"
        ]
    },
    "catalog": {
        "list": ".zj-box a",
        "orderBy": 1,
        "chapter": {
            "name": "a",
            "link": "a@attr->href"
        }
    },
